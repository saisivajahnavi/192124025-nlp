print("The temperature in the room is 25 degrees Celsius")
temperature_celsius = 25
print(f"The room temperature is {temperature_celsius} degrees Celsius")
word = "temperature"
canonical_form = word.capitalize()
print(f"The canonical form of '{word}' is '{canonical_form}'")
temperature_in_celsius = 25
temperature_in_fahrenheit = (temperature_in_celsius * 9/5 + 32)

def analyze_temperature(temp):
    if temp < 0:
        return "It is freezing!"
    elif 0 <= temp < 20:
        return "It's cool."
    else:
        return "It's warm!"

temperature = 25
analysis = analyze_temperature(temperature)
print(f"The temperature of {temperature} degrees Celsius: {analysis}")
